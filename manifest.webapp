{
  "name": "Vespa picker",
  "launch_path": "/index.html",
  "role": "system",
  "activities": {
    "pick": {
      "filters": {
        "type": ["image/*", "image/jpeg", "image/png"]
      },
      "href": "/index.html",
      "disposition": "inline",
      "returnValue": true
    }
  },
  "icons": {
    "60": "/icon/icon_60.png"
  }
}
